DOCKERHUB_USERNAME=${DOCKERHUB_USERNAME}
DOCKERHUB_TOKEN=${DOCKERHUB_TOKEN}

JENKINS_ADMIN_USER=${JENKINS_ADMIN_USER}
JENKINS_ADMIN_PASSWORD=${JENKINS_ADMIN_PASSWORD}

K8S_NAMESPACE=${K8S_NAMESPACE}
